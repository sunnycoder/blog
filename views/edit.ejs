<%-include header %>
<form method="post">
	title: 	<br>
	<input type="text" name="title" value="<%=post.title %>" disabled="disabled"> <br>
	tags: <br>
	<input type="text" name="tag1" value="<%= post.tag[0] %>" disabled="disabled"> <br>
	<input type="text" name="tag2" value="<%= post.tag[1] %>" disabled="disabled"> <br>
	<input type="text" name="tag3" value="<%= post.tag[2] %>" disabled="disabled"> <br>
	Content: <br>
	<textarea name="post"  cols="100" rows="20"> <%=post.post%> </textarea>	
	<input type="submit" value="save edit">
</form>
<%-include footer %>